User
    name Text
    password ByteString
    emailAddress Text
    verified Bool
    verifyKey Text
    resetPasswordKey Text
    UniqueUser name
    deriving Show Read Eq
Game json
    name Text
    place Text
    teamAName Text
    teamBName Text
    date UTCTime
    deriving Show Read Eq
Score json
    gameId GameId
    quarter Quarter
    teamAScore Int
    teamBScore Int
    UniqueScore gameId quarter
    deriving Show Read Eq

